// Prisma Schema Generator
generator client {
  provider = "prisma-client-js"
}

// Reference to my psql database
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// My current models for both sightings and individual Species
model PersonalSighting {
  id Int @id @default(autoincrement())
  date DateTime
  vernacular String
  sst Float
  sss Float
  shoredistance Int
  depth Int?
  count Int
  imgURL String
  longitude Float
  latitude Float
  country String?
  createdAt DateTime @default(now())
}

